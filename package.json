    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(jest-)?react-native|react-navigation|react-clone-referenced-element)"
    ],
    "moduleNameMapper": {
      "^[@./a-zA-Z0-9$_-]+\\.(png|jpg|gif)$": "RelativeImageStub"
    }
  },
  "keywords": [
    "redux"
  ],
  "license": "MIT",
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
