    "mag-service-registry": "^3.0.5",
    "react-redux": "^7.1.3",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.4",
    "redux-axios-middleware": "^4.0.0",
    "redux-batched-actions": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-loop-symbol-ponyfill": "^2.2.2",
